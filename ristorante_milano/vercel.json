{
    "version": 2,
    "builds": [
      {
        "src": "ristorante_milano/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb" }
      }
    ],
    "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "ristorante_milano/wsgi.py"
      }
    ]
  }