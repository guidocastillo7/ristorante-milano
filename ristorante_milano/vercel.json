{
    "version": 2,
    "builds": [
      {
        "src": "ristorante_milano/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "ristorante_milano/wsgi.py"
      }
    ]
  }